  "name": "postit",
  "private": true,
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.17.0",
    "babel-loader": "^6.2.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.24.1",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.17.2",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "file-loader": "^0.11.2",
    "font-loader": "^0.1.2",
    "jquery": "^3.2.1",
    "jsonwebtoken": "^7.4.1",
    "materialize-css": "^0.99.0",
    "mocha": "^3.4.2",
    "morgan": "^1.8.2",
    "nodemon": "^1.11.0",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.2.1",
    "sequelize-cli": "^2.7.0",
    "style-loader": "^0.18.2",
    "webpack": "^2.7.0"
  },
  "devDependencies": {
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-react": "^7.1.0",
    "istanbul": "^0.4.5",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^11.0.3",
    "webpack": "^2.7.0"
  },
  "nyc": {
    "extension": [
      ".jsx",
      ".es6"
    ]
  },
  "scripts": {
    "start:dev": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js -w --progress --colors --inline --content-base client/",
    "dev": "webpack-dev-server --content-base client --inline --hot",
    "test_": "nyc mocha --compilers js:babel-core/register --timeout 10000",
    "test-dev": "mocha --compilers js:babel-core/register --timeout 10000",
    "exec": "nodemon ./server/bin/www --exec babel-node ./server/app",
    "start": "nodemon ./server/bin/www --exec babel-node ./server/app",
    "coveralls_": "nyc --reporter=lcov --reporter=text-lcov npm run test_",
    "test__": "node_modules/.bin/mocha --compilers js:babel-core/register --reporter spec --slow 50 --timeout 60000",
    "cover": "node_modules/.bin/istanbul cover node_modules/mocha/bin/_mocha -- -u exports --compilers js:babel-register",
    "test": "nyc --reporter=html --reporter=text mocha --compilers js:babel-register",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "build": "webpack ./client/js/app.js"
  }
}
