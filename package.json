    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.24.1",
    "body-parser": "^1.17.2",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "jsonwebtoken": "^7.4.1",
    "mocha": "^3.4.2",
    "morgan": "^1.8.2",
    "nodemon": "^1.11.0",
    "pg": "^6.4.0",
    "pg-hstore": "^2.3.2",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "sass-loader": "^6.0.6",
